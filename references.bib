@article{hillsForagingSemanticFields2015b,
  title = {Foraging in {{Semantic Fields}}: {{How We Search Through Memory}}},
  shorttitle = {Foraging in {{Semantic Fields}}},
  author = {Hills, Thomas T. and Todd, Peter M. and Jones, Michael N.},
  year = {2015},
  volume = {7},
  pages = {513--534},
  issn = {1756-8765},
  doi = {10.1111/tops.12151},
  abstract = {When searching for concepts in memory\textemdash{}as in the verbal fluency task of naming all the animals one can think of\textemdash{}people appear to explore internal mental representations in much the same way that animals forage in physical space: searching locally within patches of information before transitioning globally between patches. However, the definition of the patches being searched in mental space is not well specified. Do we search by activating explicit predefined categories (e.g., pets) and recall items from within that category (categorical search), or do we activate and recall a connected sequence of individual items without using categorical information, with each item recalled leading to the retrieval of an associated item in a stream (associative search), or both? Using semantic representations in a search of associative memory framework and data from the animal fluency task, we tested competing hypotheses based on associative and categorical search models. Associative, but not categorical, patch transitions took longer to make than position-matched productions, suggesting that categorical transitions were not true transitions. There was also clear evidence of associative search even within categorical patch boundaries. Furthermore, most individuals' behavior was best explained by an associative search model without the addition of categorical information. Thus, our results support a search process that does not use categorical information, but for which patch boundaries shift with each recall and local search is well described by a random walk in semantic space, with switches to new regions of the semantic space when the current region is depleted.},
  copyright = {Copyright \textcopyright{} 2015 Cognitive Science Society, Inc.},
  file = {/home/nicoluarte/HDD/Zotero/storage/PE89CEBM/Hills et al. - 2015 - Foraging in Semantic Fields How We Search Through.pdf;/home/nicoluarte/HDD/Zotero/storage/IHRBECFC/tops.html},
  journal = {Topics in Cognitive Science},
  keywords = {Natural categories,Optimal foraging,Semantic space,Verbal fluency},
  language = {en},
  number = {3}
}

