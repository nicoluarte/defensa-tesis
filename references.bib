@article{raichlenEvidenceLevyWalk2014,
  title = {Evidence of {{L{\'e}vy}} Walk Foraging Patterns in Human Hunter\textendash{}Gatherers},
  author = {Raichlen, David A. and Wood, Brian M. and Gordon, Adam D. and Mabulla, Audax Z. P. and Marlowe, Frank W. and Pontzer, Herman},
