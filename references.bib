  file = {/home/nicoluarte/HDD/Zotero/storage/EQE4ULU6/Hills - 2006 - Animal Foraging and the Evolution of Goal-Directed.pdf;/home/nicoluarte/HDD/Zotero/storage/K5HL37WJ/hills2006.pdf;/home/nicoluarte/HDD/Zotero/storage/GZAX3BTH/s15516709cog0000_50.html},
  journal = {Cognitive Science},
  keywords = {ADHD,Animal foraging,Area-restricted search,Attention,Basal ganglia,Cognitive flexibility,Dopamine,Evolution,Genetic algorithm,Goal-directed behavior,OCD,Priming,Schizophrenia,Striatum},
